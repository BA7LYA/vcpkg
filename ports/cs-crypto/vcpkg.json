{
  "name": "cs-crypto",
  "version": "1.0.1",
  "description": "Interface to call existing cryptography libraries.",
  "homepage": "https://github.com/copperspice/cs_crypto",
  "license": "BSD-2-Clause",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "catch2"
    }
  ],
  "features": {
